# VaultSandbox Configuration
# Copy this file to .env and fill in your actual values
# DO NOT commit .env to version control

# Required for integration tests:

# Gateway URL (e.g., http://localhost:3000)
VAULTSANDBOX_URL=http://localhost:3000

# Your VaultSandbox API key
VAULTSANDBOX_API_KEY=your-api-key-here

# SMTP configuration for sending test emails:
SMTP_HOST=localhost
SMTP_PORT=25

# Paths to SDK repos (relative to client-interop)
CLIENT_GO_PATH=../client-go
CLIENT_NODE_PATH=../client-node
CLIENT_PYTHON_PATH=../client-python
CLIENT_JAVA_PATH=../client-java
CLIENT_DOTNET_PATH=../client-dotnet